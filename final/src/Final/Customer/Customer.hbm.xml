<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="Customer" table="customer">
		<id column="cid" name="cid" type="java.lang.Integer" />
		<property column="username" name="username" type="java.lang.String" />
		<property column="passwordhash" name="passwordhash" type="java.lang.String" />
		<property column="email" name="email" type="java.lang.String" />
	</class>
</hibernate-mapping>
